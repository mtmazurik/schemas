{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://cloudcomputingassociates/schemas/Flight.schema.json",
  "type": "object",
  "title": "sample schema for a flight JSON object",
  "required": [
    "flight"
  ],
  "properties": {
    "flight": {
      "$id": "#/properties/flight",
      "type": "object",
      "title": "The Flight Schema",
      "required": [
        "flightNumber",
        "dayOfWeek",
        "manufacturer",
        "planeType"
      ],
      "properties": {
        "flightNumber": {
          "$id": "#/properties/flight/properties/flightNumber",
          "type": "string",
          "title": "The Flightnumber Schema",
          "default": "",
          "examples": [
            "580"
          ],
          "pattern": "^(.*)$"
        },
        "dayOfWeek": {
          "$id": "#/properties/flight/properties/dayOfWeek",
          "type": "string",
          "title": "The Dayofweek Schema",
          "default": "",
          "examples": [
            "Monday"
          ],
          "pattern": "^(.*)$"
        },
        "manufacturer": {
          "$id": "#/properties/flight/properties/manufacturer",
          "type": "string",
          "title": "The Manufacturer Schema",
          "default": "",
          "examples": [
            "Boeing"
          ],
          "pattern": "^(.*)$"
        },
        "planeType": {
          "$id": "#/properties/flight/properties/planeType",
          "type": "string",
          "title": "The Planetype Schema",
          "default": "",
          "examples": [
            "777x"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}